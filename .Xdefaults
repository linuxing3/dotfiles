! urxvt color set 3
URxvt.foreground:#ffffff
URxvt.background:[90]#000000
URxvt.colorBD:Gray95
URxvt.colorUL:Green
URxvt.color1:Red2
URxvt.color4:RoyalBlue
URxvt.color5:Magenta2
URxvt.color8:Gray50
URxvt.color10:Green2
! scroll set 
URxvt.scrollBar:false
URxvt.scrollBar_right:false
URxvt.scrollBar_floating:false
URxvt.scrollstyle:plain
!URxvt.scrollWithBuffer:false
URxvt.saveLines:5000

! font set
Xft.dpi:96
URxvt.font:xft:Droid Sans Mono:size=16:antialias=true:style=Regular,xft:WenQuanYi Micro Hei Mono:size=16:antialias=true:style=Regular
URxvt.boldFont:xft:WenQuanYi MicroHei:size=16:antialias=true:style=Bold, xft:WenQuanYi Micro Hei Mono:size=16:antialias=true:style=Bold

!URxvt.letterSpace: 0

! Input Method set
URxvt.inputMethod:fcitx
URxvt.imFont: Droid Sans Mono
URxvt.preeditType: Root

! window geometry set
URxvt.geometry:80x30
URxvt.title:Rxvt-Unicode-linuxing3

! bg transparent set
URxvt.inheritPixmap: true
URxvt.tintCddolor: white
!URxvt.shading: 80

! Tab set
!URxvt.perl-ext-common: default,tabbed
URxvt.perl-ext-common: default,tabbed
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 80
URxvt.tabbed.tabbar-fg:120
URxvt.tabbed.tabbar-bg:25
URxvt.borderLess:False
URxvt.cursorBlink:True

!Rxvt.menu:/etc/X11/rxvt.menu
!Rxvt.preeditType:Root
!Rxvt.transparency:155
!

